        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        
        <Label text="Name: " textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Label text="Contact Info: " GridPane.rowIndex="1">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Label text="Room Type: " GridPane.rowIndex="2">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Label text="Number of People: " GridPane.rowIndex="3">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Label text="Check in date: " GridPane.rowIndex="4">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Label text="Check out date: " GridPane.rowIndex="5">
            <font>
                <Font name="System Bold" size="17.0" />
