        <Label text="Price: " GridPane.rowIndex="6">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        
        
        <TextField fx:id="nameField" GridPane.columnIndex="1" />
        <TextField fx:id="contactInfoField" GridPane.columnIndex="1" GridPane.rowIndex="1" />  
        <TextField fx:id="roomTypeField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <TextField fx:id="numberOfPeopleField" GridPane.columnIndex="1" GridPane.rowIndex="3" />     
        <DatePicker fx:id="checkinField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
        <DatePicker fx:id="checkoutField" GridPane.columnIndex="1" GridPane.rowIndex="5" />
        <TextField fx:id="roomPriceField" GridPane.columnIndex="1" GridPane.rowIndex="6" />

        <Button fx:id="submitButton" mnemonicParsing="false" onAction="#handleSubmitButtonEvent" prefHeight="25.0" prefWidth="85.0" text="Submit" GridPane.columnIndex="1" GridPane.rowIndex="7" />
    </children>
</GridPane>
